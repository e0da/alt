{
  "name": "alt",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Make Chrome display alt text for images again!",
  "permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "vendor/ga.js",
        "vendor/jquery.min.js",
        "lib/alt.js"
      ]
    }
  ],
  "icons": {
    "48": "images/icon48.png",
    "96": "images/icon96.png",
    "128": "images/icon128.png"
  }
}
