{
  "name": "alt",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Chrome doesn't display alt text for images. If no title is present, set the title to the alt text so you get that nice tool tip.",
  "permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "vendor/jquery.min.js",
        "lib/alt.js"
      ]
    }
  ],
  "icons": {
    "48": "images/icon48.png",
    "96": "images/icon96.png",
    "128": "images/icon128.png"
  }
}
